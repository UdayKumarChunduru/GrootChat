<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:padding="18dp"
    tools:context=".DevelopersScreen">

    <TextView
        android:id="@+id/TV1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Project Details :"
        android:textStyle="bold"
        android:fontFamily="@font/aclonica"
        android:textSize="20sp"
        android:textColor="#000000"
        android:layout_marginTop="25dp"/>
    <LinearLayout
        android:id="@+id/LL1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TV1"
        android:orientation="vertical">
        <LinearLayout
            android:layout_marginTop="3dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Project Name :"
                android:textColor="#000000"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Groot Chat"
                android:textSize="16sp"
                android:layout_marginStart="54dp"
                android:textColor="#000000"
                android:textStyle="normal"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Project Theme :"
                android:textColor="#000000"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="A Simple Chatting Application"
                android:textSize="16sp"
                android:layout_marginStart="47dp"
                android:textColor="#000000"
                android:textStyle="normal"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Note :"
                android:textColor="#000000"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="The Project Is Developed By Us (Beginners) .with the help of several web contents."
                android:textSize="16sp"
                android:layout_marginStart="110dp"
                android:textColor="#000000"
                android:textStyle="normal"/>
        </LinearLayout>
    </LinearLayout>



    <TextView
        android:id="@+id/Head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contributors :"
        android:fontFamily="@font/aclonica"
        android:gravity="left"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="#000000"
        android:layout_marginTop="34dp"
        android:layout_below="@+id/LL1" />

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/CV1"
        android:layout_marginTop="9dp"
        android:orientation="horizontal"
        android:layout_below="@+id/Head">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginTop="34dp"
            android:textStyle="bold"
            android:textSize="12sp"
            android:layout_marginStart="77.9dp"
            android:text="4th Year Student, Mechanical Engineer , RVRJC"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_marginTop="9dp"
                android:layout_width="72dp"
                android:layout_height="90dp"
                android:src="@drawable/man" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="G. Hemanth Babu"
                android:textStyle="bold"
                android:textColor="#000000"
                android:fontFamily="@font/aclonica"
                android:textSize="18sp"
                android:textAlignment="center"
                android:layout_marginStart="5dp"
                android:layout_marginTop="9dp"/>
        </LinearLayout>
        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/github"
            android:layout_marginStart="81dp"
            android:onClick="GitHub1"
            android:tag="https://github.com/GHemanth333"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/linkedin"
            android:layout_marginStart="126dp"
            android:onClick="LinkedIn1"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/webhook"
            android:layout_marginStart="171dp"
            android:onClick="PortFolio1"
            android:layout_marginTop="63dp"/>

    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="34dp"
        android:id="@+id/CV2"
        android:layout_below="@+id/CV1">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:layout_marginTop="34dp"
            android:textStyle="bold"
            android:textSize="12sp"
            android:layout_marginStart="77.9dp"
            android:text="4th Year Student, Mechanical Engineer , RVRJC"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_marginTop="9dp"
                android:layout_width="72dp"
                android:layout_height="90dp"
                android:src="@drawable/man" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Uday Kumar Chunduru"
                android:textStyle="bold"
                android:textColor="#000000"
                android:fontFamily="@font/aclonica"
                android:textSize="16sp"
                android:textAlignment="center"
                android:layout_marginStart="5dp"
                android:layout_marginTop="9dp"/>
        </LinearLayout>
        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/github"
            android:layout_marginStart="81dp"
            android:onClick="GitHub2"
            android:tag="https://github.com/UdayKumarChunduru"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/linkedin"
            android:layout_marginStart="126dp"
            android:onClick="LinkedIn2"
            android:tag="https://www.linkedin.com/in/udaykumarchunduru/"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/webhook"
            android:layout_marginStart="171dp"
            android:onClick="PortFolio2"
            android:tag="https://udaykumarchunduru.github.io/"
            android:layout_marginTop="63dp"/>

    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="34dp"
        android:id="@+id/CV3"
