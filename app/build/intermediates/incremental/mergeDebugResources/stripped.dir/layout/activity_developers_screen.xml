            android:textStyle="bold"
            android:textSize="12sp"
            android:layout_marginStart="77.9dp"
            android:text="4th Year Student, Mechanical Engineer , RVRJC"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_marginTop="9dp"
                android:layout_width="72dp"
                android:layout_height="90dp"
                android:src="@drawable/man" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="J. Yaswanth Venkat Sai"
                android:textStyle="bold"
                android:textColor="#000000"
                android:fontFamily="@font/aclonica"
                android:textSize="16sp"
                android:textAlignment="center"
                android:layout_marginStart="5dp"
                android:layout_marginTop="9dp"/>
        </LinearLayout>
        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/github"
            android:layout_marginStart="81dp"
            android:onClick="GitHub3"
            android:tag="https://github.com/YASWANTHJANJANAM"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/linkedin"
            android:layout_marginStart="126dp"
            android:onClick="LinkedIn3"
            android:layout_marginTop="63dp"/>

        <ImageView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:src="@drawable/webhook"
            android:layout_marginStart="171dp"
            android:onClick="PortFolio3"
            android:layout_marginTop="63dp"/>

    </com.google.android.material.card.MaterialCardView>

</RelativeLayout>